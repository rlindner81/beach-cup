{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Server",
      "program": "${workspaceFolder}/src/server.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--allow-all",
        "--inspect=127.0.0.1:9229",
      ],
      "outputCapture": "std",
      "attachSimplePort": 9229,
    }
  ]
}